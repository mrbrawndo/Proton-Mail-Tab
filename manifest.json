{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "2.0",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "protontab@mrbrawndo",
      "strict_min_version": "68.0"
    }
  },
  "icons": {
    "48": "icons/appicon_48.png",
    "128": "icons/appicon_128.png",
    "256": "icons/appicon_256.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "__MSG_browseractionCaption__",
    "default_icon": {
      "16": "icons/gs_16.png",
      "32": "icons/gs_32.png",
      "64": "icons/gs_64.png",
      "128": "icons/gs_128.png"
    }
  },
  "developer": {
    "name": "Mr. Brawndo",
    "url": "https://github.com/mrbrawndo"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "cookies",
    "menus",
    "activeTab"
  ]
}
